{
  "expo": {
    "name": "ZenScout",
    "slug": "zenscout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ZenScout_Icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ZenScout_SplashPage.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ZenScout_Icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.Mobileprojectsandergoethals"
    },
    "web": {
      "favicon": "./assets/ZenScout_Icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-mail-composer"
    ]
  }
}
